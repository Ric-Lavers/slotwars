

<div class="title">
  <%= image_tag("logo.png") %>
</div><!-- title -->
<div class="contatiner">

  <div class="twitter-feed">
    <a class="twitter-timeline" data-width="300" data-height="600" data-theme="dark" data-link-color="#E81C4F" href="https://twitter.com/starwars?ref_src=twsrc%5Etfw">Tweets by starwars</a> <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
  </div><!-- twitter-feed -->

  <div class="slot-machine">
    <h1>Is the force strong in you?</h1>
    <br>
    <div class="slots">
      <div class="results">

        <%= image_tag(@result[0], :class => "box") %>
        <%= image_tag(@result[1], :class => "box") %>
        <%= image_tag(@result[2], :class => "box") %>
      </div>
    </div>
    <h1 >
    <%if @score == 0 %>
      no...its not
    <%elsif @score == 5 %>
      Keep training padawan ...<%= @score%> points
    <% else %>
      yes it is.. <%= @score%> points
    <% end %>
    </h1>


    <h1 class= "purple"><%="#{current_user.score}"%></h1>
    <div class="">
      <%if current_user.score > 25 && current_user.score < 60 %>
        <%= image_tag("source.gif", :class => "gif") %>
      <%elsif current_user.score > 100 && current_user.score < 130 %>
        <%= image_tag("lady.gif", :class => "gif") %>
      <%elsif current_user.score > 200 && current_user.score < 230 %>
        <%= image_tag("kylo.gif", :class => "gif") %>
      <%end%>
    </div>
  </div><!-- slot-machine -->


</div>

  <%#=$thing.points += @score%>



